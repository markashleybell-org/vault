{
    "version": "1.0.0",
    "name": "vault",
    "private": true,
    "devDependencies": {
        "@types/bootstrap": "3.3.42",
        "@types/jest": "^24.0.11",
        "@types/jquery": "3.3.29",
        "@types/js-cookie": "2.2.1",
        "clean-webpack-plugin": "2.0.1",
        "copy-webpack-plugin": "5.0.2",
        "jest": "^24.5.0",
        "source-map-loader": "0.2.4",
        "ts-jest": "^24.0.0",
        "ts-loader": "5.3.3",
        "ts-node": "8.0.3",
        "typescript": "^3.3.4000",
        "webpack": "4.29.6",
        "webpack-cli": "3.3.0"
    },
    "dependencies": {
        "bootstrap": "3.4.1",
        "handlebars": "4.1.1",
        "jquery": "3.3.1",
        "js-cookie": "2.2.0",
        "tslib": "1.9.3"
    },
    "scripts": {
        "build": "webpack --mode=production --display-modules",
        "watch": "webpack --watch --mode=development",
        "test": "jest",
        "coverage": "jest --coverage"
