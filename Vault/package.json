{
    "version": "1.0.0",
    "name": "vault",
    "private": true,
    "devDependencies": {
        "@types/bootstrap": "3.3.42",
        "@types/jest": "^25.2.1",
        "@types/jquery": "^3.3.34",
        "@types/js-cookie": "^2.2.5",
        "clean-webpack-plugin": "^3.0.0",
        "fast-text-encoding": "^1.0.1",
        "jest": "^25.3.0",
        "source-map-loader": "0.2.4",
        "ts-jest": "^25.3.1",
        "ts-loader": "^6.2.2",
        "ts-node": "^8.8.2",
        "typescript": "^3.8.3",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11"
    },
    "dependencies": {
        "bootstrap": "3.4.1",
        "handlebars": "^4.7.6",
        "jquery": "3.4.1",
        "js-cookie": "^2.2.1",
        "tslib": "^1.11.1"
    },
    "scripts": {
        "build": "webpack --mode=production --display-modules",
        "watch": "webpack --watch --mode=production",
        "test": "jest",
        "coverage": "jest --coverage"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "jsdom",
        "testPathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ],
        "coveragePathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ]
    },
    "-vs-binding": {
        "ProjectOpened": [
            "watch"
        ]
    }
}
