{
    "version": "1.0.0",
    "name": "vault",
    "private": true,
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.2",
        "@types/bootstrap": "^5.1.12",
        "@types/jest": "^28.1.3",
        "@types/js-cookie": "^3.0.3",
        "autoprefixer": "^10.4.14",
        "clean-webpack-plugin": "^4.0.0",
        "cssnano": "^5.1.12",
        "del-cli": "^4.0.1",
        "fast-text-encoding": "^1.0.6",
        "gulp": "^4.0.2",
        "gulp-postcss": "^9.0.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^5.1.0",
        "gulp-sourcemaps": "^3.0.0",
        "jest": "^28.1.1",
        "jest-environment-jsdom": "^28.1.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.22",
        "sass": "^1.53.0",
        "source-map-loader": "^4.0.1",
        "ts-jest": "^28.0.5",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.4",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.10.0"
    },
    "dependencies": {
        "bootstrap": "^5.1.1",
        "bootstrap-icons": "^1.8.3",
        "clipboard": "^2.0.11",
        "handlebars": "^4.7.7",
        "mab-dom": "^0.12.0",
        "tslib": "^2.4.0"
    },
    "scripts": {
        "js:clean": "del-cli ./wwwroot/js/dist",
        "js:build": "webpack --mode=production",
        "css:clean": "del-cli ./wwwroot/css/dist",
        "css:build": "gulp build-css",
        "build": "run-s css:clean css:build js:clean js:build",
        "watch-js": "webpack --watch --mode=production",
        "test": "jest",
        "coverage": "jest --coverage"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "jsdom",
        "testPathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ],
        "coveragePathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ]
    },
    "-vs-binding": {
        "ProjectOpened": [
            "watch-js"
        ]
    }
}
