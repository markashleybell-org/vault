{
    "version": "1.0.0",
    "name": "vault",
    "private": true,
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.1",
        "@types/bootstrap": "^5.1.4",
        "@types/jest": "^26.0.24",
        "@types/js-cookie": "^2.2.7",
        "autoprefixer": "^10.3.4",
        "clean-webpack-plugin": "^3.0.0",
        "cssnano": "^5.0.8",
        "del-cli": "^3.0.1",
        "fast-text-encoding": "^1.0.3",
        "gulp": "^4.0.2",
        "gulp-postcss": "^9.0.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^5.0.0",
        "gulp-sourcemaps": "^3.0.0",
        "jest": "^27.1.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.3.6",
        "sass": "^1.39.2",
        "source-map-loader": "^3.0.0",
        "ts-jest": "^27.0.5",
        "ts-loader": "^9.2.5",
        "ts-node": "^10.2.1",
        "typescript": "^4.4.2",
        "webpack": "^5.52.1",
        "webpack-cli": "^4.8.0"
    },
    "dependencies": {
        "bootstrap": "^5.1.1",
        "bootstrap-icons": "^1.5.0",
        "clipboard": "^2.0.8",
        "handlebars": "^4.7.7",
        "js-cookie": "^2.2.1",
        "mab-dom": "^0.10.0",
        "tslib": "^2.3.1"
    },
    "scripts": {
        "js:clean": "del-cli ./wwwroot/js/dist",
        "js:build": "webpack --mode=production",
        "css:clean": "del-cli ./wwwroot/css/dist",
        "css:build": "gulp build-css",
        "build": "run-s css:* js:*",
        "watch-js": "webpack --watch --mode=production",
        "test": "jest",
        "coverage": "jest --coverage"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "jsdom",
        "testPathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ],
        "coveragePathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ]
    },
    "-vs-binding": {
        "ProjectOpened": [
            "watch-js"
        ]
    }
}
