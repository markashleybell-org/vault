{
    "version": "1.0.0",
    "name": "vault",
    "private": true,
    "devDependencies": {
        "@types/bootstrap": "3.3.39",
        "@types/chai": "4.1.2",
        "@types/handlebars": "4.0.36",
        "@types/jquery": "2.0.49",
        "@types/js-cookie": "2.1.0",
        "@types/mocha": "5.0.0",
        "chai": "4.1.2",
        "clean-webpack-plugin": "0.1.19",
        "copy-webpack-plugin": "4.5.1",
        "del-cli": "1.1.0",
        "istanbul": "0.4.5",
        "jsdom": "11.7.0",
        "jsdom-global": "3.0.2",
        "mocha": "5.0.5",
        "null-loader": "0.1.1",
        "nyc": "11.6.0",
        "source-map-loader": "0.2.3",
        "source-map-support": "0.5.4",
        "ts-loader": "4.1.0",
        "ts-node": "5.0.1",
        "typescript": "2.7.2",
        "webpack": "4.3.0",
        "webpack-cli": "2.0.13",
        "webpack-merge": "4.1.2"
    },
    "dependencies": {
        "bootstrap": "3.3.7",
        "handlebars": "4.0.11",
        "js-cookie": "2.2.0",
        "jquery": "2.2.1",
        "tslib": "1.9.0"
    },
    "scripts": {
        "clean": "del-cli ./Content/js/dist",
        "build": "npm run clean && webpack --config=webpack.config.build --mode=development --display-modules && webpack --config=webpack.config.test --mode=development --display-modules",
        "test": "mocha --opts ./Content/Js/tests/mocha.opts",
        "coverage": "nyc mocha --opts ./Content/Js/tests/mocha.opts"
    },
    "nyc": {
        "include": [
            "**/*.ts"
        ],
        "exclude": [
            "**/tests.ts",
            "**/Abstract.ts",
            "**/Concrete.ts",
            "**/Fake*.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "sourceMap": true,
        "instrument": true
    }
}
