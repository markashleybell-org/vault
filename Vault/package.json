{
    "version": "1.0.0",
    "name": "vault",
    "private": true,
    "devDependencies": {
        "@types/bootstrap": "^5.0.15",
        "@types/jest": "^26.0.23",
        "@types/js-cookie": "^2.2.6",
        "clean-webpack-plugin": "^3.0.0",
        "fast-text-encoding": "^1.0.3",
        "jest": "^27.0.3",
        "source-map-loader": "^3.0.0",
        "ts-jest": "^27.0.2",
        "ts-loader": "^9.2.2",
        "ts-node": "^10.0.0",
        "typescript": "^4.3.2",
        "webpack": "^5.38.1",
        "webpack-cli": "^4.7.0"
    },
    "dependencies": {
        "@tsconfig/recommended": "^1.0.1",
        "bootstrap": "^5.0.1",
        "handlebars": "^4.7.7",
        "js-cookie": "^2.2.1",
        "mab-dom": "^0.6.0",
        "tslib": "^2.2.0"
    },
    "scripts": {
        "build-dev": "webpack --mode=development",
        "build": "webpack --mode=production",
        "watch": "webpack --watch --mode=development",
        "test": "jest",
        "coverage": "jest --coverage"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "jsdom",
        "testPathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ],
        "coveragePathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ]
    },
    "-vs-binding": {
        "ProjectOpened": [
            "watch"
        ]
    }
}
