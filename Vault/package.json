{
    "version": "1.0.0",
    "name": "vault",
    "private": true,
    "devDependencies": {
        "@types/bootstrap": "3.3.42",
        "@types/jest": "^24.0.25",
        "@types/jquery": "^3.3.31",
        "@types/js-cookie": "^2.2.4",
        "clean-webpack-plugin": "^3.0.0",
        "fast-text-encoding": "1.0.0",
        "jest": "^24.9.0",
        "source-map-loader": "0.2.4",
        "ts-jest": "^24.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.4",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10"
    },
    "dependencies": {
        "bootstrap": "3.4.1",
        "handlebars": "4.5.3",
        "jquery": "3.4.1",
        "js-cookie": "^2.2.1",
        "tslib": "^1.10.0"
    },
    "scripts": {
        "build": "webpack --mode=production --display-modules",
        "watch": "webpack --watch --mode=production",
        "test": "jest",
        "coverage": "jest --coverage"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "jsdom",
        "testPathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ],
        "coveragePathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ]
    },
    "-vs-binding": {
        "ProjectOpened": [
            "watch"
        ]
    }
}
